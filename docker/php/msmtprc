# Set defaults.
defaults
# Enable or disable TLS/SSL encryption.

tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
# Setup WP account's settings.

account default
auth login
logfile /var/log/php/mail.log

host smtp.gmail.com
port 587
from newsite.test.mail@gmail.com
user newsite.test.mail@gmail.com
password newsite.test0